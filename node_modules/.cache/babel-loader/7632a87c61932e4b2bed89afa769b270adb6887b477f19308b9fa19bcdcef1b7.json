ding.value = false;\r\n//   }\r\n// };\r\n\r\n//run hook \"onMounted\"\r\nonMounted(() => {\r\n    //call method \"fetchDataproducts\"\r\n    fetchDataProducts();\r\n});\r\n</script>\r\n<style></style>"],"mappings":"AAiDA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,GAAG,MAAM,QAAQ;AACxB,SAASC,WAAW,QAAQ,kBAAkB;;;;;;;IAE9C,MAAMC,YAAY,GAAGD,WAAW,CAAC,CAAC;;IAGlC;IACA,MAAME,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMM,WAAW,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC1B,MAAMO,QAAQ,GAAGP,GAAG,CAAC,CAAC,CAAC;;IAEvB;IACA,MAAMQ,iBAAiB,GAAG,MAAAA,CAAOC,IAAI,GAAG,CAAC,KAAK;MAC5C,MAAMP,GAAG,CAACQ,GAAG,CAAC,eAAe,EAAE;QAAEC,MAAM,EAAE;UAAEF;QAAK;MAAE,CAAC,CAAC,CACjDG,IAAI,CAACC,QAAQ,IAAI;QAChB;QACAR,QAAQ,CAACS,KAAK,GAAGD,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACA,IAAI;QACxCT,WAAW,CAACQ,KAAK,GAAGD,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACC,YAAY;QACnDT,QAAQ,CAACO,KAAK,GAAGD,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACE,SAAS;MAC/C,CAAC,CAAC;IACN,CAAC;;IAED;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IAEA;IACAhB,SAAS,CAAC,MAAM;MACZ;MACAO,iBAAiB,CAAC,CAAC;IACvB,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}