data.data.data\r\n\r\n        });\r\n    }\r\n\r\n    //run hook \"onMounted\"\r\n    onMounted(() => {\r\n\r\n        //call method \"fetchDataproducts\"\r\n        fetchDataProducts();\r\n    });\r\n\r\n</script>\r\n\r\n<style lang=\"\">\r\n    \r\n</style>"],"mappings":"AAwCI,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;;AAEpC;AACA,OAAOC,GAAG,MAAM,WAAW;;AAE3B;;;;;;;;;IANA;IAOA,MAAMC,QAAQ,GAAGH,GAAG,CAAC,EAAE,CAAC;;IAExB;IACA,MAAMI,iBAAiB,GAAG,MAAAA,CAAA,KAAY;MAElC;MACA,MAAMF,GAAG,CAACG,GAAG,CAAC,eAAe,CAAC,CAE7BC,IAAI,CAACC,QAAQ,IAAI;QAEd;QACAJ,QAAQ,CAACK,KAAK,GAAGD,QAAQ,CAACE,IAAI,CAACA,IAAI,CAACA,IAAI;MAE5C,CAAC,CAAC;IACN,CAAC;;IAED;IACAR,SAAS,CAAC,MAAM;MAEZ;MACAG,iBAAiB,CAAC,CAAC;IACvB,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}